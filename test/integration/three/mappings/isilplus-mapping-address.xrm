// new mapping
output csvw

map OrganizationAddress from isilSrc {
	subject template "https://culture.ld.admin.ch/isil/{0}/address" with ISIL;

	types
		schema.PostalAddress

	properties
		schema.streetAddress from Strasse;
		schema.postalCode from PLZ;
		schema.addressLocality from Ort;
		schema.addressRegion from Kanton;
		schema.addressCountry from Land;
	
}