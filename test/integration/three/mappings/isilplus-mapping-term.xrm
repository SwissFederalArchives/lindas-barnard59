// new mapping
output csvw

map Term from isilSrc {
	subject template "https://culture.ld.admin.ch/isil/{0}" with ISIL;

	types
		schema.DefinedTerm
		admin.Isil

	properties
		schema.termCode from ISIL;
		schema.name from Institution;
		schema.url from ID with datatype xsd.anyURI;
		schema.alternateName from VZ_Code;
		schema.inDefinedTermSet constant "https://culture.ld.admin.ch/isil";
		schema.about template "https://culture.ld.admin.ch/isil/{0}/organization" with ISIL;
		admin.administrativeHistory from Verwaltungsgeschichte;
		admin.inventoryDescription from Bestandsbeschreibung;
		admin.libraryMaterials from Bibliotheksbestaende;
		admin.otherMaterials from Andere_Bestaende;
		schema.validFrom from Startdatum;
		schema.validThrough from Enddatum;
		admin.valid from Status;
		schema.replacer template "https://culture.ld.admin.ch/isil/{0}" with Aufgegangen_in;
		schema.dateModified from LastEditedOn;
		
}


